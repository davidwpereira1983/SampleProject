{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u3}] [{ThreadId:D4}] {TraceId} | {SpanId} : {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\CompanyTestProject.log",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] [{ThreadId:D4}] {TraceId} | {SpanId} : {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Main": "[TBD]"
  },
  "AzureKeyVault": {
    "Enable": false,
    "Url": "[]",
    "Prefix": "Company-TestProject-",
    "ReloadIntervalInMinutes": 10
  },
  "UseExporter": "jaeger",
  "Jaeger": {
    "ServiceName": "Company.TestProject",
    "Host": "localhost",
    "Port": 6831
  },
  "Azure": {
    "ServiceName": "Company.TestProject",
    "ConnectionString": "localhost"
  },
  "Otlp": {
    "ServiceName": "Company.TestProject",
    "Endpoint": "localhost:55680"
  }
}
